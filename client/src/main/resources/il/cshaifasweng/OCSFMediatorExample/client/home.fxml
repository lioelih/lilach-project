<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="600" prefWidth="800" spacing="15" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.HomeController">

    <Button fx:id="catalogButton" text="Catalog" />
    <Button fx:id="contactButton" text="Contact Support" />
    <Button fx:id="loginButton" text="Login" />
    <Button fx:id="registerButton" text="Register" />

    <Label style="-fx-font-size: 28px; -fx-font-weight: bold;" text="Lilach Store" />
    <Label style="-fx-font-size: 16px;" text="All your needs for flowers" />

    <ImageView fx:id="storeImage" fitHeight="200" fitWidth="300" preserveRatio="true" />

</VBox>